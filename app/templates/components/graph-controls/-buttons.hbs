{{#if graph.isDeleting}}
  {{#bs-button type="danger" onClick=(action "deleteCustomGraph" graph)}}Confirm remove{{/bs-button}}
  {{#bs-button type="success" onClick=(action "toggleConfirmRemoveCustomGraph" graph)}}Keep{{/bs-button}}
{{else}}
  {{#bs-button type="warning" onClick=(action "toggleConfirmRemoveCustomGraph" graph)}}Remove{{/bs-button}}
{{/if}}
{{#if (gt graph.order 0)}}
  {{#bs-button type="secondary" onClick=(action "moveCustomGraph" graph "up")}}Move up{{/bs-button}}
{{/if}}
{{#if (lt graph.order maxCustomGraphOrder)}}
  {{#bs-button type="secondary" onClick=(action "moveCustomGraph" graph "down")}}Move down{{/bs-button}}
{{/if}}
